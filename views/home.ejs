
<%- include("partials/header") -%>

<h1>Home</h1>
<p> <%= StartingContent %> </p>

<% postBody.forEach(function(post){ %>

  <h1><%= post.ptitle; %></h1>
  <p>
    <%= post.pbody.slice(0,100)+"..."; %>
    <a href="/posts/<%= post.ptitle %>">Read more</a>
  </p>

<% }) %>

<%- include("partials/footer") -%>
